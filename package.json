{
  "name": "atom-coverage",
  "main": "./lib/atom-coverage",
  "version": "0.0.0",
  "description": "A short description of your package",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": "atom-coverage:toggle"
  },
  "repository": "https://github.com/atom/atom-coverage",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.41.0",
    "flow-typed": "^2.0.0",
    "jest": "^19.0.2",
    "jest-babel": "^1.0.1"
  },
  "configSchema": {
    "pathToCoverageJson": {
      "title": "Path to your coverage.json generated by istanbul",
      "description": "This file is used to draw coverage highlighting into the source code editors",
      "type": "string",
      "default": "coverage/coverage-final.json",
      "order": 1
    }
  },
  "jest" : {
    "globals": {
      "atom": {}
    }
  }
}
