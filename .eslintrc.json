{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "plugins": [
        "import",
        "flowtype"
    ],
    "globals": {
      "atom": true
    },
    "rules": {
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0
    }
}
